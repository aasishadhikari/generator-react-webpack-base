doctype html
html(lang="en")
	head
		title <%= name %>
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge')


		meta(name='description', content='#{<%= description %>}')
		meta(name='viewport', content='width=device-width, initial-scale=1')

	body
		#app!= content

		each script in scripts
			script(src='#{script}', defer)
