import {render} from 'react-dom';
import React from 'react';
import {BrowserRouter} from 'react-router-dom';
<% if(redux) { %>import {Provider} from 'react-redux';
import store from './redux/store';<% } %>

const appContainer = document.getElementById('app');

<%if(redux) { %>
const app = <Provider store={store}><BrowserRouter><App basePath={config.basePath} /></BrowserRouter></Provider>;
<% } else { %>
const app = <BrowserRouter><App basePath={config.basePath} /></BrowserRouter>;
<% } %>

render(app, appContainer);
